msgid "successful_transaction_notification"
msgstr ""
"<b>Successful transaction.</b><br/>%s: <b>%s</b><br/>Order ID: <b>%s</b><br/>"
"Date: <b>%s</b>"

msgid "canceled_transaction_notification"
msgstr ""
"Cancelled transaction.<br /><br />You have interrupted the payment process "
"or run out of the maximum time limit available!<br /><br />Order ID: <b>"
"%s</b><br/>Date: <b>%s</b>"

msgid "error_transaction_notification"
msgstr ""
"Unsuccessful transaction.<br /><br />Please double-check the data you have "
"entered on the payment page. If all the data were correct, please contact "
"your card issuing bank to investigate the cause of rejection.<br /><br/>%s: "
"<b>%s</b><br/>Order ID: <b>%s</b><br/>Date: <b>%s</b>"

